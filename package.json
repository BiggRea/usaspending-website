  "scripts": {
    "start": "npx webpack serve --hot-only  --open --history-api-fallback --progress --config ./webpack/webpack.dev.config.js",
    "start:styles": "npx webpack serve --open --progress --config ./webpack/webpack.sass.config.js",
    "test": "npx jest; exit 0;",
    "lint": "npx eslint --config .eslintrc --ext .jsx,.js \"src/js/**\"; exit 0;",
    "errors": "npx eslint --config .eslintrc --ext .jsx,.js \"src/js/**\" --quiet; exit 0;",
    "dev": "npx webpack --progress --config ./webpack/webpack.dev.config.js",
    "prod": "npx webpack --progress --config ./webpack/webpack.prod.config.js",
    "travis": "npm audit --audit-level=moderate --production && npx eslint --config .eslintrc --ext .jsx,.js \"src/js/**\" && npx jest --runInBand;",
    "light-house": "npx webpack --progress --config ./webpack/webpack.prod.config.js && lhci autorun",
    "build-redirect-config": "npx babel-node scripts/buildRedirectConfig.js"
  },
